{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "gbc/contenblock",
    "title": "Content block static",
    "category": "text",
    "icon": "welcome-widgets-menus",
    "description": "Adding two texts fields and manipulate them in React",
    "keywords": [ "content", "custom", "gbc", "monkey" ],
    "version": "1",
    "textdomain": "gbc",
    "editorScript": "file:./index.js",
    "attributes": {
        "contentTitle": {
            "type": "string",
            "default": ""
        },
        "contentDescription": {
            "type": "string",
            "default": ""
        },
        "secondTitle": {
            "type": "string",
            "default": ""
        },
        "postdata": {
            "type": "object",
            "default": []
        }
    }
}