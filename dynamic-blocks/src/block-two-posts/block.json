        "contentDescription": {
            "type": "string",
            "default": ""
        },
        "secondTitle": {
            "type": "string",
            "default": ""
        }
    },
	"textdomain": "wprblocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
