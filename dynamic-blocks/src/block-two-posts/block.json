{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gbc/blockposts",
	"version": "0.1.0",
	"title": "Dynamic Block fetching posts",
	"category": "widgets",
	"icon": "welcome-widgets-menus",
	"description": "Dynamic Block fetching latests posts (articles) with their categories.",
	"example": {},
	"supports": {
		"html": false
	},
    "attributes": {
        "contentTitle": {
            "type": "string",
            "default": ""
